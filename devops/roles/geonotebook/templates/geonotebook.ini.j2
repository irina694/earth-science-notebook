[default]
vis_server = ktile
log_level = WARNING

[geoserver]
username = admin
password = geoserver
url = {{ geoserver_url | default("http://localhost:8080/geoserver") }}

[ktile]
url = {{ ktile_url | default("http://localhost:8888/ktile") }}
default_cache = ktile_default_cache

[ktile_default_cache]
name = Test
path = /tmp/stache
umask = 0000
